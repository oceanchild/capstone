<div class="tabbable" id="patient-administration" style="display: none;"> <!-- Only required for left/right tabs -->
  <ul class="nav nav-tabs bordered" id="action-tabs">
    <li class="active"><a href="#tab1" data-toggle="tab">Notes</a></li>
    <li><a href="#tab2" data-toggle="tab">Prescription</a></li>
    <li>
      <a href="#tab3" data-toggle="tab">
        <i class="hidden-phone">Primary Contact</i>
        <i class="visible-phone">Contact</i>
      </a>
    </li>
  </ul>
  <div class="tab-content well bordered">
    <div class="tab-pane fade in active" id="tab1">
      <!-- Button to trigger add note modal -->
      <a href="#add-note" id="add-note-btn" class="btn btn-primary force-tail">Add New Note</a>
      <!-- Add note modal -->
        <div id="add-note-modal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="addNote" aria-hidden="true">
          <%= render 'shared/add_note_modal_form' %>
        </div><!--/modal -->
      <ul class="nav nav-tabs nav-stacked no-tail slider-tab">
        <li class="active">
          <a class="clearfix">
            <div id="prescription-list">
            </div>
              </div>
            <% end %>
          </a>
        </li>
      </ul>
    </div>
    <div class="tab-pane fade in" id="tab2">
      <ul class="nav nav-tabs nav-stacked no-tail slider-tab">
        <li class="active">
          <a class="clearfix">
            <div class="conversation">
              <% @prescription.each do |_p|%>
                
              <% end %>
            </div>
          </a>
        </li>
      </ul>
    </div>
    <div class="tab-pane fade in" id="tab3">
      <div class="well well-small no-tail bg-eee clearfix">
        <div class="span3">
          <img class="img-rounded" src="/assets/avatars/dp10.jpg" alt="" />
        </div>
        <div class="span9">
          <span class="span12">
            <label class="form-label span2">First Name:</label>
            <input class="" type="text" readonly value="" />
          </span>
          <span class="span12">
            <label class="form-label span2">Last Name:</label>
            <input class="" type="text" readonly value="" />
          </span>
          <span class="span12">
            <label class="form-label span2">Gender:</label>
            <span class="force-tail">
              <label class="radio inline span2">
                <input type="radio" name="gender" disabled="disabled"  />
                Male
              </label>
              <label class="radio inline span2">
                <input type="radio" name="gender" disabled="disabled" checked="checked"/>
                Female
              </label>
            </span>
          </span>
          <span class="span12">
            <label class="form-label span2">Phone:</label>
            <input class="" type="text" readonly value="" />
          </span>
          <span class="span12">
            <label class="form-label span2">Address:</label>
            <input class="" type="text" readonly value="" />
          </span>
        </div>
      </div>
    </div>
  </div>
</div>